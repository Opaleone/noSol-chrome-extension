{
  "name": "noSol",
  "version": "0.1.0",
  "description": "This extension removes the solutions tab in LeetCode until timer is complete.",
  "manifest_version": 3,
  "author": "Lvarando@gmail.com",
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://leetcode.com/*"
      ],
      "js": [
        "/scripts/content.js"
      ],
      "run_at": "document_end"
    }
  ]
}