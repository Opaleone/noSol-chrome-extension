{
  "name": "noSol",
  "version": "0.1.0",
  "description": "This extension removes the solutions tab in LeetCode until timer is complete.",
  "manifest_version": 3,
  "author": "lvarando@gmail.com",
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://leetcode.com/problems/*"
      ],
      "js": [
        "/scripts/content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options/options.html"
  },
  "background": {
    "service_worker": "/scripts/background.js"
  }
}