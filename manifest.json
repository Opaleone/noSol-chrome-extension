{
  "name": "noSol",
  "version": "0.1.0",
  "description": "Removes the solutions tab in LeetCode until timer is complete.",
  "manifest_version": 3,
  "author": "lvarando@gmail.com",
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "icons": {
    "16": "./assets/noSol16.png",
    "32": "./assets/noSol32.png",
    "48": "./assets/noSol48.png",
    "128": "./assets/noSol128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://leetcode.com/problems/*"
      ],
      "js": [
        "/scripts/content.js"
      ]
    }
  ],
  "action": {
    "default_popup": "popup/popup.html"
  }
}